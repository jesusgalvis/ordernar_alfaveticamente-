#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define a 10

int main(int argc, char *argv[]) {
	
	//ordernar alfaveticamente cadena de caracteres (string)

	char c1[a],c2[a],i=0;


	printf("ingrese la palabra 1 (maximo %d caracteres) ",a);
	fgets(c1,a,stdin);
	
	printf("ingrese la palabra 2 (maximo %d caracteres) ",a);
	fgets(c2,a,stdin);
	
	if(strcmp(c1,c2)>0){
		printf("ordenando... \n1.-%s, 2.-%s",c2,c1);
	}else{
		printf("ordenando... \n1.-%s, 2.-%s",c1,c2);

	}
	
//	printf("\n%s%s",c1,c2);
	
	
	return 0;
}
